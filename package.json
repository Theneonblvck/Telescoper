{
  "name": "telescope-telegram-discovery",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "echo '⚠️  To run locally, verify you are running the backend too! Use two terminals:\n  1. npm run dev:frontend\n  2. npm run dev:backend' && npm run dev:frontend",
    "dev:frontend": "vite",
    "dev:backend": "npx tsx server.ts",
    "build": "vite build",
    "build:server": "tsc server.ts --outDir dist-server --moduleNodeNext --moduleResolution NodeNext --noEmit false",
    "start": "node dist-server/server.js",
    "preview": "vite preview"
  },
  "dependencies": {
    "@google/genai": "^1.38.0",
    "lucide-react": "^0.563.0",
    "react-dom": "^19.2.3",
    "react": "^19.2.3",
    "vite": "^7.3.1",
    "dotenv": "^17.2.3",
    "express-rate-limit": "^8.2.1",
    "@vitejs/plugin-react": "^5.1.2",
    "path": "^0.12.7",
    "helmet": "^8.1.0",
    "express": "^5.2.1",
    "url": "^0.11.4",
    "redis": "^4.6.13",
    "zustand": "^4.5.2",
    "node-cache": "^5.1.2"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "typescript": "~5.8.2",
    "tsx": "^4.7.1",
    "vite": "^6.2.0"
  }
}